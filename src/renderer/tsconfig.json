{
    "compilerOptions": {
        "target": "ESNext",
        "module": "ESNext",
        "moduleResolution": "node",
        "skipLibCheck": true,
        "lib": [
            "esnext",
            "esnext.asynciterable",
            "dom"
        ],
        "noImplicitAny": false,
        "esModuleInterop": true,
        "allowJs": true,
        "sourceMap": true,
        "strict": true,
        "noEmit": true,
        "baseUrl": ".",
        "types": [
            "@nuxt/types"
        ]
    }
}
